<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Results"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00F8F0F0"
    android:backgroundTint="#DCD2D2"
    tools:context=".Results">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/barmenubar8"
        android:layout_width="398dp"
        android:layout_height="80dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="671dp"
        android:background="#CAEDCB"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/barmenubar4"
        android:layout_width="376dp"
        android:layout_height="143dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="498dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="16dp"
        android:background="#CAEDCB"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SearchView
        android:id="@+id/BarSearch"
        android:layout_width="246dp"
        android:layout_height="22dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="605dp"
        android:contentDescription="searchbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/barmenubar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="671dp"
            android:background="#81CA84"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme" />
    </SearchView>

    <ImageButton
        android:id="@+id/btnMenuResults"
        android:layout_width="73dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="304dp"
        android:layout_marginBottom="583dp"
        android:backgroundTint="#81CA84"
        android:contentDescription="menubar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_more" />

    <ImageButton
        android:id="@+id/btnFunfacts2"
        style="@android:style/Widget.Holo.Light.ImageButton"
        android:layout_width="69dp"
        android:layout_height="58dp"
        android:layout_marginStart="282dp"
        android:layout_marginTop="489dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="27dp"
        android:contentDescription="funfactsbutton"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.703"
        app:srcCompat="@drawable/book_fill" />

    <ImageButton
        android:id="@+id/btnLogin2"
        android:layout_width="102dp"
        android:layout_height="84dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="530dp"
        android:layout_marginEnd="261dp"
        android:layout_marginBottom="43dp"
        android:contentDescription="login_button"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/login_circle_line" />

    <ImageButton
        android:id="@+id/btnClimateChange2"
        style="@android:style/Widget.Holo.Light.ImageButton"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_marginStart="166dp"
        android:layout_marginTop="530dp"
        android:layout_marginEnd="149dp"
        android:layout_marginBottom="35dp"
        android:contentDescription="climatechange_button"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.835"
        app:srcCompat="@drawable/earth_line" />

    <View
        android:id="@+id/txtResultsView"
        android:layout_width="210dp"
        android:layout_height="183dp"
        android:layout_marginTop="116dp"
        android:layout_marginBottom="304dp"
        android:contentDescription="resultsview"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextResults"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:contentDescription="results"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="results"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.654"
        tools:ignore="LabelFor" />
</androidx.constraintlayout.widget.ConstraintLayout>